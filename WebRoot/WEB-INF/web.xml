<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>UploadSingleFileServlet1</servlet-name>
    <servlet-class>com.kdyzm.servlet.UploadSingleFileServlet1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UploadMultipleFilesServlet1</servlet-name>
    <servlet-class>com.kdyzm.servlet.UploadMultipleFilesServlet1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CatalogBreakUp</servlet-name>
    <servlet-class>com.kdyzm.servlet.CatalogBreakUp</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>UploadSingleFileServlet1</servlet-name>
    <url-pattern>/uploadSingleFileServlet1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UploadMultipleFilesServlet1</servlet-name>
    <url-pattern>/uploadMultipleFilesServlet1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CatalogBreakUp</servlet-name>
    <url-pattern>/catalogBreakUp</url-pattern>
  </servlet-mapping>
	
	
</web-app>